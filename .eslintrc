{
  "extends": "eslint:recommended",
  "globals": {
    "angular": 1
  },
  "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "eqeqeq": [
            "error",
            "always"
        ],
        "space-infix-ops": [
            "error",
            {"int32Hint": false}
        ],
        "max-len": [
            "error",
            100,
            2
        ],
        "no-new-object": "error",
        "no-array-constructor": "error",
        "brace-style": "error"
    }
}
